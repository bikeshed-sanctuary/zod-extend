{
  "name": "zod-extend",
  "version": "0.1.3",
  "type": "module",
  "description": "A utility to create standardized zod extension functions",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run"
  },
  "keywords": [
    "zod",
    "extend",
    "schema",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "zod": "^3.0.0"
  },
  "dependencies": {
    "universal-symbols": "^0.1.2"
  }
}
